TRANSMITTER PICO W PIN LAYOUT:

KEYPAD: 
	COLUMN	4	GP 	5
	COLUMN	3	GP 	6
	COLUMN	2	GP 	7
	COLUMN	1	GP 	8
	ROW	4	GP 	9
	ROW	3	GP 	10
	ROW	2	GP 	11
	ROW	1	GP 	12

RGB:    
	RED	GP 	15
	GREEN	GP 	16
	BLUE	GP 	17

RFID MRC522:
	SDA	GP	1
	SCK	GP	2
	MOSI	GP	3
	MISO	GP	4
	IRQ	NOT USED
	GND	GND
	RST	GP	0
	3.3V	3.3V

RECIEVER PICO W PIN LAYOUT:

LCD:
	SCL	GP	5
	SDA	GP	4
	VCC	VBUS
	GND	GND

BUZZER 1:
	+	GP	14
	GND	GND

BUZZER 2:
	+	GP	15
	GND	GND
